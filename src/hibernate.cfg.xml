	<mapping resource="com/juncsoft/KLJY/InHospitalCase/Mouth/entity/config.hbn.xml"/>
	<!-- 外科住院病历实体映射 -->
	<mapping resource="com/juncsoft/KLJY/InHospitalCase/Surgery/entity/config.hbn.xml"/>
	<!-- 病程记录实体映射 -->
	<mapping resource="com/juncsoft/KLJY/InHospitalCase/CourseRecording/entity/config.hbn.xml"/>
	
	<!-- 课题实体映射 -->
	<mapping resource="com/juncsoft/KLJY/system/Research/entity/config.hbn.xml"/>
	<!-- 病人入选课题及随访映射 -->
	<mapping resource="com/juncsoft/KLJY/ResearchFollowup/entity/config.hbn.xml"/>
	<!-- 会员分组随访模板实体映射 -->
	<mapping resource="com/juncsoft/KLJY/system/MemberGroup/config.hbn.xml"/>
	<!-- 阳性症状检测表-->
	<mapping resource="com/juncsoft/KLJY/followVisit/entity/config.hbn.xml"/>
	<!-- 会员-->
	<mapping resource="com/juncsoft/KLJY/member/entity/config.hbn.xml"/>
	
	<mapping resource="com/juncsoft/KLJY/InHospitalCase/Liver/PresentHistory/config.hbn.xml" />
	
	<!-- 麻醉科实体映射 -->
	<mapping resource="com/juncsoft/KLJY/InHospitalCase/Anaesthetization/entity/anaesthetizationRec.hbn.xml"/>
	
	<!-- 新生儿实体映射 -->
	<mapping resource="com/juncsoft/KLJY/InHospitalCase/BabyRec/entity/config.hbn.xml"/>
	
	<!-- 检查报告实体映射 -->
	<mapping resource="com/juncsoft/KLJY/InHospitalCase/CheckReport/entity/config.hbn.xml"/>
	
	<!-- 病案首页 -->
	<mapping resource="com/juncsoft/KLJY/InHospitalCase/HistoryIndexPage/entity/config.hbn.xml" />
	
	<!-- 申请单 -->
	<mapping resource="com/juncsoft/KLJY/InHospitalCase/Apply/entity/config.hbn.xml" />
