	<!--外网病人 和医生网上留言 -->
	<mapping resource="com/juncsoft/KLJY/patientanddoctoroperator/entity/config.hbn.xml"/>
	<!-- 新增会员组别，会员责任制划分 -->
	<mapping resource="com/juncsoft/KLJY/membergrounp/entity/config.hbn.xml"/>
	<!-- 麦迪 瑞科 -->
	<mapping resource="com/juncsoft/KLJY/plan/entity/config.hbn.xml"/>
	<mapping resource="com/juncsoft/KLJY/checkreport/entity/config.hbn.xml"/>
	
	<!-- 短信管理 -->
	<mapping resource="com/juncsoft/KLJY/message/entity/config.hbn.xml"/>
	
	<!-- 艾滋病史 -->
	<mapping resource="com/juncsoft/KLJY/InHospitalCase/aids/entity/config.hbn.xml"/>
	
	<!-- 发热病史 -->
	<mapping resource="com/juncsoft/KLJY/InHospitalCase/Fever/PresentHistoryNew/config.hbn.xml"/>

	<!-- 甲流病史 -->
	<mapping resource="com/juncsoft/KLJY/InHospitalCase/H1N1/entity/config.hbn.xml"/>
</session-factory>

</hibernate-configuration>