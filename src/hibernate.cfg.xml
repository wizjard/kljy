<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
          "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">

<hibernate-configuration>

<session-factory>
	
	<property name="connection.datasource">java:comp/env/jdbc/juncsoftDB</property>
	
	<property name="dialect">
		org.hibernate.dialect.SQLServerDialect
	</property>
	
	<property name="show_sql">false</property>
	<property name="format_sql">false</property>
	
	<!-- 系统模块实体映射 -->
	<mapping resource="com/juncsoft/KLJY/system/module/entity/config.hbn.xml"/>
	<!-- Common模块实体映射 -->
	<mapping resource="com/juncsoft/KLJY/Public/entity/config.hbn.xml"/>
	<!-- 字典实体映射 -->
	<mapping resource="com/juncsoft/KLJY/system/dict/entity/config.hbn.xml"/>
	<!-- 用户实体映射 -->
	<mapping resource="com/juncsoft/KLJY/user/entity/config.hbn.xml"/>
	<!-- 病人实体映射 -->
	<mapping resource="com/juncsoft/KLJY/patient/entity/config.hbn.xml"/>
	
	<!-- 住院病历主表实体映射 -->
	<mapping resource="com/juncsoft/KLJY/InHospitalCase/CaseMaster/entity/config.hbn.xml"/>
	<!-- 默认普通住院病历实体映射 -->
	<mapping resource="com/juncsoft/KLJY/InHospitalCase/Default/entity/config.hbn.xml"/>
	<!-- 肝病住院病历实体映射 -->
	<mapping resource="com/juncsoft/KLJY/InHospitalCase/Liver/entity/config.hbn.xml"/>
	<!-- 中医住院病历实体映射 -->
	<mapping resource="com/juncsoft/KLJY/InHospitalCase/TraditionalChineseMedical/entity/config.hbn.xml"/>
