	<mapping
		resource="com/juncsoft/KLJY/InHospitalCase/pitemScore/ItemScore/entity/config.hbn.xml" />
	<!-- 公告管理 -->
	<mapping resource="com/juncsoft/KLJY/system/notice/entity/config.hbn.xml"/>
	<mapping resource="com/juncsoft/KLJY/system/picture/entity/config.hbn.xml"/>
	
	<!-- 最新现病史 -->
	<mapping resource="com/juncsoft/KLJY/InHospitalCase/Liver/PresentHistoryNew/config.hbn.xml"/>
