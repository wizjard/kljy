<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.juncsoft.KLJY.system.notice.entity">
	<class name="Notice" table="t_InHsp_Notice">
		<id column="id" name="id">
			<generator class="native"></generator>
		</id>
		<property name="noticeNam"/>
		<property name="noticeContent"/>
		<property name="noticeTim"/>
		<property name="noticeAuthor"/>
		<property name="noticeCompany"/>
		<property name="filePathList"/>
		<property name="typeName"/>
	</class>
</hibernate-mapping>
