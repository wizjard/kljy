<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.juncsoft.KLJY.InHospitalCase.Anaesthetization.entity">
	<!-- 日常病程实体映射 -->
	<class name="AnaesthetizationRec" table="t_CourseRec_Anaesthetization">
		<id column="id" name="id">
			<generator class="native"></generator>
		</id>
		<property name="caseId"/>
		<property name="patientId"/>
		<property name="time"/>
		<property name="title"/>
		<property name="content"/>
		<property name="submiter"/>
		<property name="submitTime"/>
		<property name="checker"/>
		<property name="checkTime"/>
		<property name="verifyStatus"/>
		<property name="createTime"/>
		<property name="modifyTime"/>
	</class>
</hibernate-mapping>
