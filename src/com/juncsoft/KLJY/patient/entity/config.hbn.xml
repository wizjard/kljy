		<property name="marrageStatus"/>
		<property name="marrageStatus0"/>
		<property name="educationLv"/>
		<property name="educationLv0"/>
		<property name="mobilePhone"/>
		<property name="email"/>
		<property name="workUnit"/>
		<property name="workUnitAddr"/>
		<property name="workUnitTel"/>
		<property name="workUnitPostCode"/>
		<property name="contacterName"/>
		<property name="contacterRelationship"/>
		<property name="contacterRelationship0"/>
		<property name="contacterTel"/>
		<property name="createDate"/>
		<property name="modifyDate"/>
		<property name="currentWardCode"/>
		<property name="patientId"/>
		<property name="outDeptCode"/>
		<property name="isOut"/>
		<property name="diagGrounp"/>
	</class>
	
	<class name="SysZdUserBelong" table="SYS_ZD_UserBelong">
		<id column="id" name="id">
			<generator class="native"></generator>
		</id>
		<property name="pcode"/>
		<property name="code"/>
		<property name="name"/>
		<property name="pycode"/>
		<property name="memo"/>
	</class>
</hibernate-mapping>
