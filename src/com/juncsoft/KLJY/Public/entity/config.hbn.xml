<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.juncsoft.KLJY.Public.entity">
	<class name="IllsEntity" table="SYS_ZD_Ills">
		<id column="ID" name="id" type="java.lang.Long">
			<generator class="native"></generator>
		</id>
		<property name="code" column="CODE"/>
		<property name="name" column="NAME"/>
		<property name="pycode" column="PYCODE"/>
	</class>
	
	<class name="DepartmentHISEntity" table="SYS_HIS_DepartmentHISEntity">
		<id column="ID" name="id" type="java.lang.Long">
			<generator class="native"></generator>
		</id>
		<property name="deptCode" column="deptCode"/>
		<property name="deptName" column="deptName"/>
		<property name="flag" column="flag"/>
		<property name="outType" column="outType"/>
		<property name="inHospitalType" column="inHospitalType"/>
	</class>
</hibernate-mapping>
